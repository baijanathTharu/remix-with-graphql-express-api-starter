{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development remix build && run-p dev:*",
    "dev:node": "cross-env NODE_ENV=development nodemon ./build/index.js --watch ./build/index.js",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "migrate:dev": "prisma migrate dev"
  },
  "dependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/graphql-modules-preset": "^2.3.7",
    "@graphql-codegen/typescript-resolvers": "^2.5.4",
    "@graphql-yoga/node": "^0.0.1-canary-ae94a57.0",
    "@prisma/client": "^3.10.0",
    "@remix-run/express": "^1.2.3",
    "@remix-run/react": "^1.2.3",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "graphql": "^16.3.0",
    "graphql-modules": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "eslint": "^8.9.0",
    "nodemon": "^2.0.15",
    "prisma": "^3.10.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
